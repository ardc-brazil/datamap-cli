# DataMap CLI Configuration
# Copy this file to .env and fill in your actual values

# API Authentication
DATAMAP_API_KEY=your-api-key-here
DATAMAP_API_SECRET=your-api-secret-here

# API Configuration
DATAMAP_API_BASE_URL=https://datamap.pcs.usp.br/api/v1
DATAMAP_TIMEOUT=30
DATAMAP_RETRY_ATTEMPTS=3

# Optional: User and Tenancy Information
# DATAMAP_USER_ID=your-user-id-here
# DATAMAP_TENANCIES=your-tenancy-info-here

# Logging Configuration
DATAMAP_LOG_LEVEL=INFO
DATAMAP_LOG_FORMAT=json

# Output Configuration
DATAMAP_OUTPUT_FORMAT=table
DATAMAP_COLOR_OUTPUT=true
