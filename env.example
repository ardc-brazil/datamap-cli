# DataMap CLI Configuration
# Copy this file to .env and fill in your actual values

# Required: Your DataMap API credentials
DATAMAP_API_KEY=your-api-key-here
DATAMAP_API_SECRET=your-api-secret-here

# Optional: API Configuration
DATAMAP_API_BASE_URL=https://datamap.pcs.usp.br/api/v1
DATAMAP_TIMEOUT=30
DATAMAP_RETRY_ATTEMPTS=3

# Optional: User Information
DATAMAP_USER_ID=your-user-id
DATAMAP_TENANCIES=your-tenancy-info

# Optional: Logging Configuration
DATAMAP_LOG_LEVEL=INFO
DATAMAP_LOG_FORMAT=json

# Optional: Output Configuration
DATAMAP_OUTPUT_FORMAT=table
DATAMAP_COLOR_OUTPUT=true

# Optional: Download Configuration
DATAMAP_DOWNLOAD_CONCURRENCY=3
DATAMAP_CHUNK_SIZE=8192 